{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "globalize@1.7.0",
      "destination": "wwwroot/lib/globalize/",
      "files": [
        "globalize.min.js",
        "globalize.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation-globalize@latest",
      "destination": "wwwroot/lib/jquery-validation-globalize/",
      "files": [
        "jquery.validate.globalize.min.js",
        "jquery.validate.globalize.js"
      ]
    },
    {
      "library": "bootstrap-icons@1.11.1",
      "destination": "wwwroot\\lib\\bootstrap-icons"
    }
  ]
}